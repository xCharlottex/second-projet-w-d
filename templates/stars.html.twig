{% include 'head.html.twig' %}
<body>
    {% for star in stars %}
        {% if star.published %}
        <article style="background-color: {{ star.color }}">
            <h1>{{ star.title }}</h1>
            <img src="{{ star.image }}">
            <p>{{ star.publishedDate|date("m/d/Y") }}</p>
            <a href= {{ path ('star', {'id': star.id})  }}>click</a>

        </article>
        {% endif %}
    {% endfor %}

